                <img class="content__user-ranking__users__paragraf-2__user__image__user" src="https://geinou-navi.com/wp-content/uploads/2018/07/ishiharasatomi2-5.jpg" alt="アイコン">
              </div>
              <div class="content__user-ranking__users__paragraf-2__user__info">
                <p class="content__user-ranking__users__paragraf-2__user__info__name"><%= @users[i].last_name %> <%= @users[i].first_name %></p>
                <p class="content__user-ranking__users__paragraf-2__user__info__position"><%= @users[i].position %></p>
                <p class="content__user-ranking__users__paragraf-2__user__info__like-count">週間 Liked ユーザー数: <span class="content__user-ranking__users__paragraf-2__user__info__like-count__num">2347</span></p>
              </div>
              <% if user_signed_in? %>
                <% if @users[i].id != current_user.id %>
                  <% if current_user.following?(@users[i]) %>
                    <div class="content__user-ranking__users__paragraf-2__user__follow">
                      <div class="follow-btn" id="user-follow<%= @users[i].id %>">
                        <%= link_to "/unfollow/#{@users[i].id}", class: "destroy-relation", data: @users[i].id, method: :delete do %>
                          <img src="images/following-btn.png">
                        <% end %>
                      </div>
                    </div>
                  <% else %>
                    <div class="content__user-ranking__users__paragraf-2__user__follow">
                      <div class="follow-btn" id="user-follow<%= @users[i].id %>">
                        <%= link_to "/follow/#{@users[i].id}", class: "create-relation", data: @users[i].id, method: :post do %>
                          <img src="images/follow-btn.png">
                        <% end %>
                      </div>
                    </div>
                  <% end %>
                <% end %>
              <% end %>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
