<div class="content">
  <!-- トップアイキャッチ -->
  <div class="content__eyecatch">
    <!-- メイン -->
    <%= link_to "/picks/#{@top_picks[0].id}", class: "content__eyecatch__link" do %>
      <div class="content__eyecatch__main" style="background-image: url(<%= @top_picks[0].image %>);">
        <div class="content__eyecatch__main__pick-count">
          <span class="content__eyecatch__main__pick-count__num"><%= @top_picks[0].comments.length %></span>
          Picks
        </div>
        <div class="content__eyecatch__main__title">
          <%= @top_picks[0].title %>
        </div>
        <div class="content__eyecatch__main__info">
          <p class="content__eyecatch__main__info__media"><%= @top_picks[0].source %></p>
          <p class="content__eyecatch__main__info__time">4時間前</p>
        </div>
      </div>
    <% end %>
    <div class="content__eyecatch__sub">
      <!-- サブ１ -->
      <%= link_to "/picks/#{@top_picks[1].id}", class: "content__eyecatch__link" do %>
        <div class="content__eyecatch__sub-1" style="background-image: url(<%= @top_picks[1].image %>);">
