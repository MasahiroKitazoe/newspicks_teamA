            <div class="content__eyecatch__sub-1__special__logo">
              特集
            </div>
            ライブリポート
          </div>
          <div class="content__eyecatch__sub-1__title">
            <%= @top_picks[1].title %>
          </div>
        </div>
      <% end %>
      <!-- サブ２ -->
